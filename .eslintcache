Dialogo>,\r\n      document.getElementById(\"editar\")\r\n    );\r\n  };\r\n\r\n  const Icono = (categoria) => {\r\n    switch (categoria) {\r\n      case \"Comida\":\r\n        return <FastfoodIcon fontSize=\"small\" style={{ color: green[500] }} />;\r\n      case \"Diversion\":\r\n        return (\r\n          <SportsEsportsIcon fontSize=\"small\" style={{ color: green[500] }} />\r\n        );\r\n      case \"Hogar\":\r\n        return <HomeIcon fontSize=\"small\" style={{ color: green[500] }} />;\r\n      case \"Cuentas y Pagos\":\r\n        return <LocalAtmIcon fontSize=\"small\" style={{ color: green[500] }} />;\r\n      case \"Transporte\":\r\n        return <CommuteIcon fontSize=\"small\" style={{ color: green[500] }} />;\r\n      case \"Ropa\":\r\n        return (\r\n          <ImageAspectRatioIcon\r\n            fontSize=\"small\"\r\n            style={{ color: green[500] }}\r\n          />\r\n        );\r\n      case \"Salud e Higiene\":\r\n        return (\r\n          <LocalHospitalIcon fontSize=\"small\" style={{ color: green[500] }} />\r\n        );\r\n      case \"Compras \":\r\n        return <LocalMallIcon fontSize=\"small\" style={{ color: green[500] }} />;\r\n      default:\r\n        return <LocalAtmIcon fontSize=\"small\" style={{ color: green[500] }} />;\r\n    }\r\n  };\r\n\r\n  function fechaDeGasto(fecha) {\r\n    var dia = fecha.getDate(),\r\n      mes = fecha.getMonth(),\r\n      año = fecha.getFullYear();\r\n    var meses = [\r\n      \"Enero\",\r\n      \"Febrero\",\r\n      \"Marzo\",\r\n      \"Abril\",\r\n      \"Mayo\",\r\n      \"Junio\",\r\n      \"Julio\",\r\n      \"Agosto\",\r\n      \"Septiembre\",\r\n      \"Otubre\",\r\n      \"Noviembre\",\r\n      \"Diciembre\",\r\n    ];\r\n    var pdia = dia,\r\n      pmes = meses[mes],\r\n      paño = año;\r\n    return pdia + \" de \" + pmes + \" del \" + paño;\r\n  }\r\n\r\n  var gastot = 0;\r\n  const GastoResultado = (gasto) => {\r\n    const gastoActual = parseFloat(gasto);\r\n    gastot = gastot + gastoActual;\r\n    return ReactDOM.render(\r\n      <p>TOTAL GASTADO:{gastot}$</p>,\r\n      document.getElementById(\"gasto\")\r\n    );\r\n  };\r\n\r\n  const Gastos = React.lazy(() => import(\"./Gastos\")); // Carga diferida\r\n\r\n  return (\r\n    <Fragment>\r\n\r\n      <Suspense\r\n        fallback={\r\n          <Box display=\"flex\" justifyContent=\"center\" m={1} p={1} bgcolor=\"background.paper\">\r\n          <CircularProgress />\r\n        </Box>\r\n        }\r\n      >\r\n        <Gastos\r\n          links={props.links}\r\n          GastoResultado={GastoResultado}\r\n          fechaDeGasto={fechaDeGasto}\r\n          Icono={Icono}\r\n          modal={modal}\r\n          eliminar={eliminar}\r\n        />\r\n      </Suspense>\r\n      <Box mt={2}>\r\n        <Alert severity=\"info\">\r\n          <b id=\"gasto\"> </b>\r\n        </Alert>\r\n      </Box>\r\n      <div id=\"editar\"></div>\r\n    </Fragment>\r\n  );\r\n}\r\n","C:\\xampp\\htdocs\\appgastos\\src\\Components\\GastosCategoria.jsx",[],"C:\\xampp\\htdocs\\appgastos\\src\\FormAddGasto.jsx",[],"C:\\xampp\\htdocs\\appgastos\\src\\Components\\Gastos.jsx",[],"C:\\xampp\\htdocs\\appgastos\\src\\ComponentsForm\\InputSelect.jsx",[],"C:\\xampp\\htdocs\\appgastos\\src\\ComponentsForm\\InputDate.jsx",[],"C:\\xampp\\htdocs\\appgastos\\src\\ComponentsForm\\Input.jsx",[],"C:\\xampp\\htdocs\\appgastos\\src\\Components\\Dialogo.jsx",[],{"ruleId":"87","replacedBy":"88"},{"ruleId":"89","replacedBy":"90"},{"ruleId":"91","severity":1,"message":"92","line":2,"column":1,"nodeType":"93","endLine":12,"endColumn":2},{"ruleId":"94","severity":1,"message":"95","line":1,"column":36,"nodeType":"96","messageId":"97","endLine":1,"endColumn":44},{"ruleId":"98","severity":1,"message":"99","line":88,"column":8,"nodeType":"100","endLine":88,"endColumn":10,"suggestions":"101"},{"ruleId":"94","severity":1,"message":"102","line":1,"column":17,"nodeType":"96","messageId":"97","endLine":1,"endColumn":26},{"ruleId":"9